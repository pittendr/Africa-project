<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0EAD6"
    android:weightSum = "3"
    android:gravity="center">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_weight="2"
        android:layout_marginTop="25dp"
        android:weightSum="5"
        android:gravity="center"
    >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.5"
            android:gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:id="@+id/rectangle1"
                android:layout_gravity="center"
                android:padding="2dp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:id="@+id/rectangle2"
                android:padding="2dp"
                android:layout_gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.5"
            android:gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:layout_gravity="center"
                android:id="@+id/rectangle3"
                android:padding="2dp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:id="@+id/rectangle4"
                android:padding="2dp"
                android:layout_gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.5"
            android:gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:layout_gravity="center"
                android:id="@+id/rectangle5"
                android:padding="2dp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:id="@+id/rectangle6"
                android:padding="2dp"
                android:layout_gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.5"
            android:gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:layout_gravity="center"
                android:id="@+id/rectangle7"
                android:padding="2dp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:padding="2dp"
                android:id="@+id/rectangle8"
                android:layout_gravity="center"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="0.5"
            android:gravity="center">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:layout_gravity="center"
                android:id="@+id/rectangle9"
                android:padding="2dp"/>

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/flashrectangle1"
                android:padding="2dp"
                android:id="@+id/rectangle10"
                android:layout_gravity="center"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:weightSum="2"
        >



        <FrameLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#BFD584">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/noPestButton"
                android:layout_weight="1"
                android:src="@drawable/nopest"
                android:scaleType="fitXY"
                android:adjustViewBounds="true"
                android:padding="0dp"
                android:onClick="noPestClick"/>


        </FrameLayout>

        <FrameLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="#B80F10">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/yesPestButton"
                android:layout_weight="1"
                android:src="@drawable/yespest"
                android:scaleType="fitXY"
                android:adjustViewBounds="true"
                android:padding="0dp"
                android:onClick="yesPestClick"/>




        </FrameLayout>

    </LinearLayout>

</LinearLayout>